{
  "name": "example_3",
  "version": "0.1.0",
  "description": "example showing headless firefox + web driver + json server tests",
  "main": "index.js",
  "repository": "https://github.com/plitwinski/automated-tests-presentation",
  "scripts": {
    "selenium-test": "jest",
    "start": "concurrently --kill-others \"npm run start-web-server\" \"npm run start-api\"",
    "start-web-server": "browserify --debug --extension=js -o src/bundle.js src/index.js && http-server -p 8000",
    "start-api": "json-server --watch test_data/db.json"
  },
  "author": "plitwinski",
  "license": "ISC",
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "presets": [
            "es2015",
            "react"
          ]
        }
      ]
    ]
  },
  "jest": {
    "verbose": true,
	"testURL": "http://localhost/"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-beta.35",
    "babel-core": "^6.26.0",
    "babel-jest": "^22.0.4",
    "babel-plugin-syntax-async-functions": "^6.13.0",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babelify": "^8.0.0",
    "browserify": "^14.5.0",
    "concurrently": "^3.5.1",
    "firefox-profile": "^1.1.0",
    "geckodriver": "^1.10.0",
    "http-server": "^14.1.1",
    "jest": "^22.0.3",
    "json-server": "^0.12.1",
    "regenerator-runtime": "^0.11.1",
    "selenium-webdriver": "^3.6.0"
  },
  "dependencies": {
    "axios": "^0.21.2",
    "jest": "^20.0.0",
    "prop-types": "^15.6.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-redux": "^5.0.6",
    "redux": "^3.7.2"
  }
}
