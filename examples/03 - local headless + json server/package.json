{
  "name": "example_3",
  "version": "0.0.1",
  "description": "example showing headless firefox + web driver + json server tests",
  "main": "index.js",
  "scripts": {
    "test": "json-server --watch test/db.json",
    "start": "concurrently --kill-others \"npm run startWebServer\" \"npm run startApi\"",
    "startWebServer": "browserify --debug --extension=js -o src/bundle.js src/index.js && http-server -p 8000",
    "startApi": "json-server --watch test/db.json"
  },
  "author": "plitwinski",
  "license": "ISC",
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "presets": [
            "es2015",
            "react"
          ]
        }
      ]
    ]
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babelify": "^8.0.0",
    "browserify": "^14.5.0",
    "concurrently": "^3.5.1",
    "http-server": "^0.10.0",
    "json-server": "^0.12.1",
    "selenium-webdriver": "^3.6.0"
  },
  "dependencies": {
    "axios": "^0.17.1",
    "prop-types": "^15.6.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-redux": "^5.0.6",
    "redux": "^3.7.2"
  }
}
